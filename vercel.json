{
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/admin/(.*)", "dest": "/static/admin/index.html" },
      { "src": "/blog/(.*)", "dest": "/blog/index.html" },
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/content/blog/(.*)", "dest": "/blog/index.html" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
